    {"name":"ADDING_TOKEN","description":"Adding token"}
  ],
  "paths": {
    "/api/users/assignMatch": {
      "put": {
        "tags": ["MATCH"],
        "summary": "Assigniment",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Done............."
          }
        }
      }
      }, 
       "/api/users/getMatch": {
      "get": {
        "tags": ["MATCH"],
        "summary": "get your result",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Here it is."
          }
        }
      }
      },
    "/api/users/interest": {
      "post": {
        "tags": ["INTERESTS"],
        "summary": "Registration of int",
        "description": "Registration",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "properties": {
                "sports": {
                  "type": "string",
                  "example": "football"
                },
                "values": {
                  "type": "string",
                  "example": "confidence"
                },
                "skincolor": {
                  "type": "string",
                  "example": "brown"
                },
                "height": {
                  "type": "string",
                  "example": "tall"
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Recorded."
          }
        }
      }
    },
     "/api/users/addToken": {
      "post": {
        "tags": ["ADDING_TOKEN"],
        "summary": "If api requires token use this",
        "description": "If api requires token use this",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "properties": {
                "registeredName": {
                  "type": "string",
                  "example": "DAVID"
                }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Token added you can continue."
          }
        }
      }
    },
    "/api/users": {
      "get": {
        "tags": ["USER"],
        "summary": "All users",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "All"
          }
        }
      },
      "post": {
        "tags": ["USER"],
        "summary": "Registration",
        "description": "Registration",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "properties": {
              "fullName": {
                "type": "string",
                "example": "Icyamamare David"
              },
              "darassa": {
                "type": "string",
                "example": "Y2A"
              },
              "social": {
                "type": "string",
                "example": "baganineza_david"
              },
              "gender": {
                "type": "string",
                "example": "M"
              },
              "movie": {
                "type": "string",
                "example": "romance"
              },
              "interest": {
                "type": "object",
                "properties": {
                  "music": {
                    "type": "string",
                    "example": "hiphop"
                  },
                  "sports": {
                    "type": "string",
                    "example": "football"
                  },
                  "values": {
                    "type": "string",
                    "example": "confidence"
                  },
                  "creativity": {
                    "type": "string",
                    "example": "art"
                  },
                  "searching": {
                    "type": "string",
                    "example": "relationship"
                  },
                  "char": {
                    "type": "object",
                    "properties": {
                      "skincolor": {
                        "type": "string",
                        "example": "brown"
                      },
                      "height": {
                        "type": "string",
                        "example": "tall"
                      }
                    }
                  }
                }
              },
              "otherInt": {
                "type": "object",
                "properties": {
                  "social": {
                    "type": "string",
                    "example": "tsapp"
                  },
                  "news": {
                    "type": "string",
                    "example": "showbizz"
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Recorded."
          }
        }
      }
    },
    "/api/users/{id}": {
      "get": {
        "tags": ["USER"],
        "summary": "get user by id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "User found"
          }
        }
      }
    }
  },
